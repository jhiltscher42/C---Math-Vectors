#ifndef MATH_VECTOR
#define MATH_VECTOR

class mathVector{
 protected:
  double x,y;
 public:
  mathVector(double X, double Y);
  static mathVector& createFromPolar(double angle, double magnitude);
  double getX();
  double getY();
  double getAngle();
  double getMagnitude();

  mathVector& operator+(mathVector& op);
  mathVector& operator*(double magnitude);
};


#endif
